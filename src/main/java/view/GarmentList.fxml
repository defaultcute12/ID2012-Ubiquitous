<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.view.GarmentListController">
    <children>
        <TableView fx:id="garment" layoutX="7.0" minWidth="-Infinity" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
              <TableColumn fx:id="RFID" editable="true" prefWidth="45.0" text="RFID" />
              <TableColumn fx:id="MaxWashTemp" editable="true" prefWidth="125.0" text="MaxWashTemp" />
                <TableColumn fx:id="ColorBleedResist" editable="true" prefWidth="128.0" text="ColorBleedResist" />
                <TableColumn fx:id="Weight" editable="true" prefWidth="67.0" text="Weight" />
                <TableColumn fx:id="SpinningLimit" editable="true" prefWidth="122.0" text="SpinningLimit" />
                <TableColumn fx:id="YarnTwist" editable="true" prefWidth="82.0" text="YarnTwist" />
                <TableColumn fx:id="SelectedGarments" prefWidth="75.0" resizable="false" text="Selected" />
            </columns>
          </TableView>
      <Button mnemonicParsing="false" onAction="#handleGarmentsSelectedForWashing" text="Select For Washing" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0" />
    </children>

</AnchorPane>
